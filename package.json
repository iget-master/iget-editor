{
  "name": "@iget/editor",
  "version": "12.0.0",
  "description": "WYSIWYG Editor for Angular Material applications",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "build:package": "sh ./scripts/build.sh",
    "docs": "compodoc && npm run publish:docs",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:scripts": "eslint ./scripts",
    "ng": "ng",
    "postbuild": "npm run build:package",
    "prebuild": "npm run lint",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prerelease": "npm run build",
    "publish:docs": "node ./scripts/publishdocs.js",
    "release": "sh ./scripts/release.sh",
    "start": "ng serve -o --port 5000 --host 0.0.0.0",
    "serve:docs": "compodoc --serve --open --output=\"docs/\"",
    "test": "ng test",
    "test:single-run": "ng test --watch false"
  },
  "keywords": [
    "angular-editor",
    "angular-wysiwyg-editor",
    "wysiwyg-editor",
    "ngx-editor",
    "iget-editor",
    "angular2-wysiwyg-editor",
    "angular4-wysiwyg-editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iget-master/iget-editor.git"
  },
  "bugs": {
    "url": "https://github.com/iget-master/iget-editor/issues"
  },
  "homepage": "https://github.com/iget-master/iget-editor",
  "peerDependencies": {
    "@angular/cdk": "^11.0.0",
    "@angular/material": "^11.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.15",
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "^12.2.15",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@compodoc/compodoc": "^1.1.18",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.42",
    "codelyzer": "^6.0.2",
    "gh-pages": "^2.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^10.1.2",
    "postcss": "^8.4.5",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "~4.0.8",
    "zone.js": "~0.10.2"
  }
}
