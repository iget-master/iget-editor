{
  "name": "@iget/editor",
  "version": "0.0.0-development",
  "description": "WYSIWYG Editor for Angular Material applications",
  "license": "MIT",
  "scripts": {
    "build": "ng build --configuration production",
    "build:package": "sh ./scripts/build.sh",
    "docs:build": "compodoc -p tsconfig.compodoc.json --includes=extras -d dist/docs --disableGraph --disableCoverage --disablePrivate --disableInternal --disableLifeCycleHooks --disableProtected",
    "docs:publish": "node ./scripts/publishdocs.js",
    "docs:serve": "npm run docs:build -- --serve --open",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:scripts": "eslint ./scripts",
    "ng": "ng",
    "postbuild": "npm run build:package",
    "postversion": "cp -r package.json ../..",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prerelease": "npm run build",
    "release": "sh ./scripts/release.sh",
    "start": "ng serve -o --port 5000 --host 0.0.0.0",
    "test": "ng test",
    "test:single-run": "ng test --watch false",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "angular-editor",
    "angular-wysiwyg-editor",
    "wysiwyg-editor",
    "ngx-editor",
    "iget-editor",
    "angular2-wysiwyg-editor",
    "angular4-wysiwyg-editor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iget-master/iget-editor.git"
  },
  "bugs": {
    "url": "https://github.com/iget-master/iget-editor/issues"
  },
  "homepage": "https://github.com/iget-master/iget-editor",
  "peerDependencies": {
    "@angular/cdk": "^13.0.0",
    "@angular/material": "^13.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.1",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.2.0",
    "@angular/animations": "^13.2.1",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "^13.2.1",
    "@angular/common": "^13.2.1",
    "@angular/compiler": "^13.2.1",
    "@angular/compiler-cli": "^13.2.1",
    "@angular/core": "^13.2.1",
    "@angular/forms": "^13.2.1",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^13.2.1",
    "@angular/platform-browser-dynamic": "^13.2.1",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@compodoc/compodoc": "^1.1.18",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.42",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "gh-pages": "^2.2.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.2.1",
    "postcss": "^8.4.5",
    "protractor": "~7.0.0",
    "rxjs": "^7.5.4",
    "semantic-release": "^19.0.2",
    "tslib": "^2.3.1",
    "typescript": "~4.5.5",
    "zone.js": "~0.11.4"
  }
}
